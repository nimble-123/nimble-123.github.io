<core:FragmentDefinition
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<l:Grid
		defaultSpan="L12 M12 S12"
		width="auto">
		<l:content>
			<f:SimpleForm
				minWidth="1024"
				maxContainerCols="2"
				editable="false"
				layout="ResponsiveGridLayout"
				title="{i18n>Overview_Title}"
				labelSpanL="2"
				labelSpanM="2"
				emptySpanL="5"
				emptySpanM="5"
				columnsL="1"
				columnsM="1">
				<f:content>
					<Label text="{i18n>Overview_Progress}" />					
					<Text text="{progress}" />
					<Label text="{i18n>Overview_GateOne}" />
					<ProgressIndicator id="GateOneProgessIndicator" width="300px" height="18px" percentValue="100" displayValue="100%" />
					<Label text="{i18n>Overview_GateTwo}" />
					<ProgressIndicator id="GateTwoProgessIndicator" width="300px" height="18px" percentValue="89" displayValue="89%" />
					<Label text="{i18n>Overview_GateThree}" />
					<ProgressIndicator id="GateThreeProgessIndicator" width="300px" height="18px" percentValue="66" displayValue="66%" />
					<Label text="{i18n>Overview_GateFour}" />
					<ProgressIndicator id="GateFourProgessIndicator" width="300px" height="18px" percentValue="5" displayValue="5%" />		
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>